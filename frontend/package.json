{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "moment": "^2.19.1",
    "path-to-regexp": "^2.1.0",
    "react": "^16.1.1",
    "react-debounce-input": "^3.1.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-preset-react-app": "3.0.3",
    "enzyme": "3.1.1",
    "enzyme-adapter-react-16": "1.0.4",
    "enzyme-to-json": "3.0.1",
    "jest": "21.2.1",
    "jest-fetch-mock": "^1.3.3",
    "raf": "3.3.2",
    "react-test-renderer": "16.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill --setupFiles='./src/jest.setup.js'",
    "test-coverage": "react-scripts test --env=jsdom --coverage --setupTestFrameworkScriptFile=raf/polyfill --setupFiles='./src/jest.setup.js'",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/jest.config.js",
      "!src/jest.setup.js",
      "!src/registerServiceWorker.js"
    ]
  }
